					fazenda_ammount_text.text = \"x\" + String(fazenda_ammount)
					update_money()
				5:
					bank+=musica_value
					musica_ammount-=1
					musica_value_text.text = \"Valor: \\n$\" + String(musica_value)
					musica_ammount_text.text = \"x\" + String(musica_ammount)
