				7:
					bank-=startup_value
					startup_ammount+=1
					startup_value_text.text = \"Valor: \\n$\" + String(startup_value)
					startup_ammount_text.text = \"x\" + String(startup_ammount)
					update_money()
			
		2: 
			match tipo:
				1:
					bank+=bitcoin_value
					bitcoin_ammount -= 1
					bitcoin_value_text.text = \"Valor: \\n$\" + String(bitcoin_value)
					bitcoin_ammount_text.text = \"x\" + String(bitcoin_ammount)
